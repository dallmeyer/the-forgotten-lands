(defun bird-bob-func ((arg0 sparticle-system) (arg1 sparticle-cpuinfo) (arg2 vector))
  (set! (-> arg2 y)
        (+ (-> (the-as process-drawable (-> arg1 key proc)) root trans y)
           (* -2048.0 (sin (* 218.45334 (the float (mod (-> *display* base-frame-counter) 300)))))))
  0
  (none))

